{
  "name": "audiobook-electron",
  "productName": "audiobook-electron",
  "version": "1.0.0",
  "description": "Audiobook player developed as electron app",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --force"
  },
  "keywords": [],
  "author": "Deseteral <github.com/deseteral>",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "audiobook_electron"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "audiobookelectron"
      }
    }
  },
  "dependencies": {
    "@types/electron-devtools-installer": "2.0.2",
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "electron-compile": "6.4.2",
    "electron-devtools-installer": "2.2.1",
    "electron-squirrel-startup": "1.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "tslib": "1.8.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "electron-forge": "4.1.5",
    "electron-prebuilt-compile": "1.7.9",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "tslint-react": "3.2.0",
    "typescript": "2.6.2"
  }
}
